// 用于自动重启服务端代码修改
{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules",
        ".eslintrc",
        "client",
        "build/webpack.config.client.js",
        "public"
    ],
    "verbose": true,
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json ejs"
}